# Copy this file to terraform.tfvars and customize the values

# AWS region for deployment
aws_region = "us-east-1"

# Name of the MWAA environment (must be unique in your account)
environment_name = "flowrs-mwaa"

# Airflow version (see AWS MWAA docs for available versions)
airflow_version = "2.10.3"

# Environment class (mw1.small, mw1.medium, mw1.large, mw1.xlarge, mw1.2xlarge)
environment_class = "mw1.small"

# Worker configuration
max_workers = 2
min_workers = 1

# Webserver access mode (PUBLIC_ONLY or PRIVATE_ONLY)
webserver_access_mode = "PUBLIC_ONLY"
